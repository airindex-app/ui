{
  "entryPoints": ["src/types/components.ts", "src/types/hooks.ts", "src/types/stitches.ts"],
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "githubPages": false,
  "sort": ["source-order"],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "hideGenerator": true,
  "disableSources": false,
  "includeVersion": true,
  "categorizeByGroup": true,
  "defaultCategory": "Components",
  "categoryOrder": [
    "Components",
    "Configuration",
    "Themes", 
    "Design Tokens",
    "Utilities",
    "Animations",
    "*"
  ],
  "groupOrder": [
    "Interfaces",
    "*"
  ],
  "exclude": [
    "playground/**/*",
    "dist/**/*",
    "node_modules/**/*"
  ],
  "skipErrorChecking": true,
  "customFooterHtml": "Generated from TypeScript definitions in <code>src/types/</code> directory"
} 