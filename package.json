      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/cosmo-au/design-system/issues"
  },
  "devDependencies": {
    "@cosmo-au/eslint-config-cosmo": "^1.1.3",
    "@stitches/react": "^1.2.6",
    "@types/react": "^17.0.39",
    "eslint": "^8.8.0",
    "microbundle": "^0.14.2",
    "next": "^12.0.10",
    "nookies": "^2.5.2",
    "parcel": "^2.2.1",
    "phosphor-react": "^1.4.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.5.5",
    "usehooks-ts": "^2.3.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://design.cosmogroup.io",
  "keywords": [
