{
  "name": "@cosmoau/ui",
  "description": "Cosmo UI is a React (Typescript) library for internal use that provides components for creating user interfaces, rapidly spinning up landing pages, and enforcing consistency across every product.",
  "version": "1.2.2",
  "author": "Jackson Dolman <mail@dolmios.com>",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "bugs": {
    "url": "https://github.com/cosmoau/ui/issues"
  },
  "dependencies": {
    "@stitches/react": "1.2.8",
    "next": "13.0.5",
    "phosphor-react": "1.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "typescript": "^4.9.4",
    "usehooks-ts": "2.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "eslint": "^8.29.0",
    "eslint-config-dolmios": "^1.3.2",
    "eslint-config-next": "^13.0.6",
    "parcel": "^2.8.1",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.1",
    "process": "^0.11.10",
    "tslib": "^2.4.1",
    "util": "^0.12.5"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/cosmoau/ui",
  "keywords": [
    "component",
    "components",
    "cosmo",
    "design",
    "framework",
    "frontend",
    "next",
    "nextjs",
    "react",
    "react-component",
    "stitches",
    "stitches-react",
    "ui",
    "user interface"
  ],
  "license": "GPL-3.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "prettier": "eslint-config-dolmios/.prettierrc.json",
  "repository": {
    "type": "git",
    "url": "git://github.com/cosmoau/ui.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc",
    "clean": "rm -rf dist lib",
    "dev": "parcel playground/index.html --dist-dir playground/dist --open --no-cache",
    "lint": "eslint src --ext .ts,.tsx",
    "prettier": "prettier --check --write src",
    "tidy": "pnpm lint && pnpm prettier"
  },
  "sideEffects": false,
  "source": "src/index.tsx",
  "types": "./dist/esm/index.d.ts"
}
