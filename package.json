{
  "name": "@cosmoau/ui",
  "description": "Cosmo's internal component library - Next.js + TypeScript.",
  "version": "1.3.1",
  "author": "Jackson Dolman <mail@dolmios.com>",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "bugs": {
    "url": "https://github.com/cosmoau/ui/issues"
  },
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "phosphor-react": "^1.4.1",
    "react-hot-toast": "^2.4.0",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.1",
    "@types/react": "^18.0.28",
    "eslint": "^8.36.0",
    "eslint-config-dolmios": "^1.4.2",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "next": "^13.2.4",
    "parcel": "^2.8.3",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "util": "^0.12.5"
  },
  "eslintConfig": {
    "extends": [
      "dolmios"
    ],
    "plugins": [
      "sort-keys-fix"
    ],
    "rules": {
      "sort-keys-fix/sort-keys-fix": "warn"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/cosmoau/ui",
  "keywords": [
    "component",
    "components",
    "cosmo",
    "design",
    "framework",
    "frontend",
    "next",
    "nextjs",
    "react",
    "react-component",
    "stitches",
    "stitches-react",
    "ui",
    "user interface"
  ],
  "license": "GPL-3.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "peerDependencies": {
    "next": "^13.1.1",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "usehooks-ts": "^2.9.1"
  },
  "prettier": "eslint-config-dolmios/configs/prettier",
  "repository": {
    "type": "git",
    "url": "git://github.com/cosmoau/ui.git"
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "clean": "rm -rf dist lib",
    "dev": "echo '‚ö°Ô∏è Fetching device IP for development...' && ipconfig getifaddr en0 && echo 'ü´°' && parcel playground/index.html --dist-dir playground/dist --open --no-cache --port 420",
    "lint": "eslint src --ext .ts,.tsx",
    "prettier": "prettier --check --write src",
    "tidy": "pnpm lint && pnpm prettier"
  },
  "sideEffects": false,
  "source": "src/index.ts",
  "types": "dist/esm/index.d.ts"
}
