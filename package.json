    "@stitches/react": ">=1.2.6",
    "next": ">=12.1.0",
    "nookies": ">=2.0.0",
    "phosphor-react": ">=1.3.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "prettier": "@cosmo-au/eslint-config-cosmo/.prettierrc.json",
  "repository": "https://github.com/cosmo-au/design-system",
  "scripts": {
    "build": "yarn lint && microbundle --jsx React.createElement --compress --format esm,cjs,modern",
    "clean": "prettier --write '**/*.{js,css,jsx,tsx,ts}' '!node_modules/**' '!.git/**' '!dist/**'",
    "dev": "parcel test/index.html --dist-dir test/dist --open",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "watch": "microbundle watch --jsx React.createElement"
  },
  "source": "src/index.tsx",
  "types": "dist/index.d.ts",
  "packageManager": "yarn@3.1.1"
