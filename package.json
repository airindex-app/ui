{
  "name": "@cosmo-au/design-system",
  "description": "The goal of the Cosmo Design System is to streamline our teams ability to build and maintain Cosmo projects, enforcing strict component APIs and performance parameters.",
  "version": "0.1.0",
  "private": false,
  "author": "Jackson Dolman <jackson@cosmogroup.io>",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/cosmo-au/design-system/issues"
  },
  "devDependencies": {
    "@cosmo-au/eslint-config-cosmo": "^1.1.0",
    "@stitches/react": "^1.2.6",
    "@types/react": "^17.0.38",
    "eslint": "^8.8.0",
    "js-confetti": "^0.10.2",
    "microbundle": "^0.14.2",
    "next": "^12.0.10",
    "nookies": "^2.5.2",
    "np": "^7.6.0",
    "parcel": "^2.2.1",
    "phosphor-react": "^1.4.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5",
    "usehooks-ts": "^2.3.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://design.cosmogroup.io",
  "keywords": [
    "component",
    "components",
    "cosmo",
    "design",
    "framework",
    "frontend",
    "next",
    "nextjs",
    "react",
    "react-component",
    "stitches",
    "stitches-react",
    "ui",
    "user interface"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.module.js",
  "np": {
    "branch": "main",
    "message": "release: v%s"
  },
  "peerDependencies": {
    "@stitches/react": ">=1.2.6",
    "next": ">=12.0.10",
    "react": ">=17.0.2"
  },
  "prettier": "@cosmo-au/eslint-config-cosmo/.prettierrc.json",
  "repository": "https://github.com/cosmo-au/design-system",
  "scripts": {
    "build": "yarn lint && microbundle  --jsx 'React.createElement' --jsxImportSource react --format modern,cjs",
    "clean": "prettier --write '**/*.{js,css,jsx,tsx,ts}' '!node_modules/**' '!.git/**' '!dist/**'",
    "dev": "parcel test/index.html",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "release": "np",
    "watch": "microbundle watch --jsx 'React.createElement' --jsxImportSource react --format modern,cjs"
  },
  "source": "src/index.tsx",
  "types": "dist/index.d.ts"
}
