{
  "name": "@cosmoau/ui",
  "description": "Cosmo's internal component library - Next.js + TypeScript.",
  "version": "1.5.28",
  "author": "Jackson Dolman <mail@dolmios.com>",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/cosmoau/ui/issues"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.1",
    "@stitches/react": "^1.2.8"
  },
  "devDependencies": {
    "@phosphor-icons/react": "^2.0.9",
    "@size-limit/preset-big-lib": "^8.2.4",
    "@types/google.maps": "^3.53.4",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.12",
    "eslint": "^8.42.0",
    "eslint-config-dolmios": "^1.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "lodash": "^4.17.21",
    "next": "^13.4.5",
    "parcel": "2.8.3",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "usehooks-ts": "^2.9.1",
    "util": "^0.12.5"
  },
  "eslintConfig": {
    "extends": [
      "dolmios"
    ],
    "plugins": [
      "sort-keys-fix"
    ],
    "rules": {
      "sort-keys-fix/sort-keys-fix": "warn",
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/cosmoau/ui",
  "keywords": [
    "component",
    "components",
    "cosmo",
    "design",
    "framework",
    "frontend",
    "next",
    "nextjs",
    "react",
    "react-component",
    "stitches",
    "stitches-react",
    "ui",
    "user interface"
  ],
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "peerDependencies": {
    "lodash": "^4.17.21",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1"
  },
  "prettier": "eslint-config-dolmios/configs/prettier",
  "repository": {
    "type": "git",
    "url": "git://github.com/cosmoau/ui.git"
  },
  "scripts": {
    "build": "rm -rf dist lib && pnpm build:esm && pnpm build:cjs && size-limit --json > size-limit.json",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "dev": "echo '‚ö°Ô∏è Fetching device IP for development...' && ipconfig getifaddr en0 && echo 'ü´°' && parcel playground/index.html --dist-dir playground/dist --open --no-cache --port 420",
    "kill": "lsof -ti tcp:420 | xargs kill",
    "lint": "eslint src playground --ext .ts,.tsx --fix",
    "prettier": "prettier --check --write src playground",
    "tidy": "pnpm lint && pnpm prettier"
  },
  "size-limit": [
    {
      "path": "dist/esm/index.js",
      "limit": "1 KB",
      "name": "esm",
      "webpack": false
    },
    {
      "path": "dist/cjs/index.js",
      "limit": "849 KB",
      "name": "cjs"
    },
    {
      "path": [
        "dist/esm/index.js",
        "dist/cjs/index.js"
      ],
      "limit": "850 KB",
      "name": "both",
      "running": "pnpm build"
    }
  ],
  "source": "src/index.tsx",
  "types": "dist/esm/index.d.ts"
}
