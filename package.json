      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/cosmo-au/design-system/issues"
  },
  "devDependencies": {
    "@cosmo-au/eslint-config-cosmo": "^1.1.9",
    "@stitches/react": "^1.2.7",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "eslint": "^8.10.0",
    "microbundle": "^0.14.2",
    "next": "^12.1.0",
    "nookies": "^2.5.2",
    "parcel": "^2.3.2",
    "phosphor-react": "^1.4.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "4.1.6",
    "usehooks-ts": "^2.4.2"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://design.cosmogroup.io",
  "keywords": [
    "component",
