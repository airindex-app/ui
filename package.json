{
  "name": "@airindex-app/ui",
  "description": "AirIndex's internal component library, built with React, and Stitches.",
  "version": "0.2.0",
  "author": "Jackson Dolman <mail@dolmios.com>",
  "browserslist": {
    "production": [
      "defaults",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/airindex-app/ui/issues"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@radix-ui/react-icons": "^1.3.2",
    "dayjs": "^1.11.18",
    "fast-sort": "^3.4.1",
    "react-hot-toast": "^2.6.0",
    "react-wrap-balancer": "^1.1.1"
  },
  "devDependencies": {
    "@stitches/react": "^1.2.8",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.35.0",
    "eslint-config-dolmios": "^2.0.12",
    "bun-types": "^1.2.19",
    "esbuild": "^0.23.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/airindex-app/ui",
  "keywords": [
    "component",
    "components",
    "cosmo",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "stitches",
    "stitches-react",
    "ui",
    "user interface"
  ],
  "license": "MIT",
  "module": "dist/index.js",
  "peerDependencies": {
    "@stitches/react": "^1.2.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "prettier": "eslint-config-dolmios/configs/prettier",
  "repository": {
    "type": "git",
    "url": "git://github.com/airindex-app/ui.git"
  },
  "scripts": {
    "build": "bun run tidy && bun run prebuild && bun run build:js && bun run build:types",
    "build:js": "bun build ./src/index.ts --outdir ./dist --format=esm --target=browser --jsx=react --minify --external react --external react-dom --external @stitches/react",
    "build:types": "tsc -p tsconfig.build.json",
    "bump": "bunx npm-check-updates -u && bun install && bun run tidy && bun run build",
    "dev": "echo '⚡️ Starting Bun preview on http://localhost:420' && bun run build:js && bun run playground:dev",
    "playground:dev": "bun run playground/dev.ts",
    "documentation": "node scripts/generate-docs.js",
    "kill": "lsof -ti tcp:420 | xargs kill",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "prebuild": "rm -rf dist .parcel-cache playground/dist",
    "prettier": "prettier --check --write src",
    "tidy": "bun run lint && bun run prettier",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "source": "src/index.ts",
  "types": "dist/types.d.ts"
}